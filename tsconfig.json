{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "es2016"
    ],
    "moduleResolution": "node",
    "outDir": "dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
